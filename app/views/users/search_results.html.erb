
<h1>Search Results</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Website</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user['name'] %></td>
        <td><%= user['email'] %></td>
        <td><%= user['website'] %></td>
      </tr>
    <% end %>
  </tbody>
</table>
